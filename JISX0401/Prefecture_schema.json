{
  "$schema": "https://json-schema.org/draft/2019-09/schema",
  "$comment": "2020/08/08 @HirMtsd",
  "title": "Prefcture_list.json schema",
  "description": "Prefecture list (JIS X 0401:1973, ISO 3166-2:JP) json schema",
  "type": "object",
  "properties": {
    "prefectures": {
      "description": "都道府県コード",
      "type": "array",
      "uniquness": true,
      "items": {
        "type": "object",
        "properties": {
          "code": {
              "$ref": "../common/LocalPublicEntityCode.json#/definitions/prefecture_code"
          },
          "name": {
            "description": "都道府県名",
            "type": "string",
            "pattern": "^(北海道|東京都|(大阪|京都)府|(青森|岩手|宮城|秋田|山形|福島|茨城|栃木|群馬|埼玉|千葉|神奈川|新潟|富山|石川|福井|山梨|長野|岐阜|静岡|愛知|三重|滋賀|兵庫|奈良|和歌山|鳥取|島根|岡山|広島|山口|徳島|香川|愛媛|高知|福岡|佐賀|長崎|熊本|大分|宮崎|鹿児島|沖縄)県)$"
          },
          "kana_name": {
            "description": "都道府県名カナ",
            "type": "object",
            "properties": {
              "half_upper": {
                "description": "半角カナ、拗音・促音は大書き",
                "type": "string"
              },
              "full_lower": {
                "description": "全角カナ、拗音・促音は小書き",
                "type": "string"
              }
            }
          },
          "en_name": {
            "description": "都道府県名英字",
            "type": "object",
            "properties": {
              "en": {
                "description": "ISO 3166-2:JPのen",
                "type": [
                  "string",
                  "null"
                ]
              },
              "ja": {
                "description": "ISO 3166-2:JPのjp",
                "type": "string"
              }
            }
          },
          "start_date": {
            "description": "適用開始日",
            "type": "string",
            "format": "date"
          },
          "end_date": {
            "description": "適用終了日",
            "anyOf": [
              {
                "type": "string",
                "format": "date"
              },
              {
                "type": "null"
              }
            ]
          },
          "note": {
            "description": "備考 編集上の注記",
            "type": [
              "string",
              "null"
            ]
          }
        },
        "required": [
          "code"
        ]
      }
    }
  }
}